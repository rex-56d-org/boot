---
id: INDEX
layout: 'index.hbs'
meta:
  title: 'INDDEX'
  description: '$DESCRIPTION'
  keywords: '$KEYWORDS'
---

<div class="container">
	<div class="page-header">
		<h1>INDEX</h1>
	</div>
	<div class="row">
		<div class="col-md-12 ">
			<table class="table table-striped table-hover">
				<thead>
					<tr>
						<th>画面ID</th>
						<th>画面名</th>
						<th>URL</th>
					</tr>
				</thead>
				<tbody>
					{{# each pages }}
						{{#isnt data.id 'INDEX' }}
						<tr>
							<td>{{ data.id }}</td>
							<td>{{ data.meta.title }}</td>
							<td><a href="{{ relativeLink }}">{{ relativeLink }}</a></td>
						</tr>
						{{/isnt }}
					{{/ each }}
				</tbody>
			</table>
		</div>
	</div>
</div>
